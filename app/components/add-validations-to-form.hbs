{{#if this.canShowRdfForm}}
  <RdfForm
    @groupClass="au-o-grid__item"
    @form={{this.builderForm}}
    @graphs={{this.graphs}}
    @formStore={{this.builderStore}}
    @forceShowErrors={{true}}
    @useNewListingLayout={{true}}
    @sourceNode={{this.graphs.sourceNode}}
  />
{{else}}
  <AuLoader />
{{/if}}