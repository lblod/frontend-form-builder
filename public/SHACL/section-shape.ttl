###############################
# Section shape for V2 forms ##
###############################

form:Section
  a sh:NodeShape ;
  sh:targetClass form:Section ;
  sh:property
    [
      sh:path sh:name ;
      sh:datatype xsd:string ;
      sh:minCount 1 ;
      sh:maxCount 1 ;
    ],
    [
      sh:path sh:order ;
      sh:datatype xsd:integer ;
      sh:minCount 1 ;
      sh:maxCount 1 ;
    ] .
